{
  "name": "typescript-jasmine-istanbul-boilerplate",
  "version": "1.0.0",
  "description": "An example node project to produce coverage of TypeScript sources using Jasmine.",
  "scripts": {
    "test": "ts-node ./spec-runner",
    "test:coverage": "ts-node node_modules/istanbul/lib/cli.js cover ./spec-runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Izhaki/Typescript-Jasmine-Istanbul-Boilerplate.git"
  },
  "keywords": [
    "typescript",
    "jasmine",
    "istanbul",
    "sourcemaps",
    "node",
    "npm scripts",
    "tsc"
  ],
  "author": "Izhaki",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "@types/node": "^7.0.13",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine": "2.5.3",
    "jasmine-spec-reporter": "^4.0.0",
    "ts-node": "3.0.2",
    "typescript": "2.3.0"
  },
  "bugs": {
    "url": "https://github.com/Izhaki/Typescript-Jasmine-Istanbul-Boilerplate/issues"
  },
  "homepage": "https://github.com/Izhaki/Typescript-Jasmine-Istanbul-Boilerplate#readme"
